/* components/TarBar/TarBar.wxss */
@import "../style/iconfont-weapp-icon.wxss";

.yy-icon {
  width: 40rpx;
  height: 40rpx;
}

.yy-tarbar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  background: white;
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
  border-top: 1px solid #F2F2F2;
}

.yy-tar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 28rpx;
  color: #A4A4A4;
}